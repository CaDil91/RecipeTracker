{
  "name": "foodbudget",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "start:mock": "cross-env EXPO_PUBLIC_USE_MSW=true expo start",
    "android": "expo start --android",
    "android:mock": "cross-env EXPO_PUBLIC_USE_MSW=true expo start --android",
    "ios": "expo start --ios",
    "ios:mock": "cross-env EXPO_PUBLIC_USE_MSW=true expo start --ios",
    "web": "expo start --web",
    "web:mock": "cross-env EXPO_PUBLIC_USE_MSW=true expo start --web",
    "build:web": "expo export:web",
    "build:web:prod": "cross-env NODE_ENV=production expo export:web",
    "msw:init": "npx msw init public/ --save",
    "test": "jest",
    "test:coverage": "jest --coverage --coverageDirectory=coverage",
    "predeploy": "npm run build:web:prod && node -e \"require('fs').writeFileSync('dist/.nojekyll', '')\"",
    "deploy": "gh-pages -d dist --dotfiles -f"
  },
  "dependencies": {
    "@expo/metro-runtime": "~5.0.4",
    "@expo/vector-icons": "^14.1.0",
    "@react-navigation/bottom-tabs": "^7.4.7",
    "@react-navigation/native": "^7.1.14",
    "@react-navigation/native-stack": "^7.3.21",
    "@react-navigation/stack": "^7.4.2",
    "@tanstack/react-query": "^5.90.2",
    "expo": "53.0.22",
    "expo-font": "~13.3.2",
    "expo-status-bar": "~2.2.3",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.63.0",
    "react-native": "^0.79.5",
    "react-native-paper": "^5.14.5",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-web": "^0.20.0",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.0.10",
    "cross-env": "^10.0.0",
    "fast-text-encoding": "^1.0.6",
    "gh-pages": "^6.3.0",
    "jest": "^29.7.0",
    "jest-expo": "~53.0.10",
    "jest-fixed-jsdom": "^0.0.10",
    "msw": "^2.11.3",
    "react-native-url-polyfill": "^3.0.0",
    "typescript": "~5.8.3"
  },
  "private": true,
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
